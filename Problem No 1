//Reverse the array
// Topic Array
// Question link https://leetcode.com/problems/reverse-string/
// Solution
// Time Complexity O(n)
// Space complexity O(1)
class Solution {
public:
    void reverseString(vector<char>& s) {
        int n=s.size();
        for(int i=0;i<n/2;i++)
        {
            swap(s[i],s[n-i-1]);
        }
    }
};
